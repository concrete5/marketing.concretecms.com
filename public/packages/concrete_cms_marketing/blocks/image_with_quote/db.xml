<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="btImageWithQuote">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="fID" type="integer" size="10"/>
        <field name="name" type="string" size="255"/>
        <field name="background" type="string" size="255"/>
        <field name="alignment" type="string" size="255"/>
        <field name="quote" type="text"/>
    </table>
</schema>
